<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>College_check</servlet-name>
        <servlet-class>controller.College_check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>College_details</servlet-name>
        <servlet-class>controller.College_details</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentRegis</servlet-name>
        <servlet-class>controller.StudentRegis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertComplain</servlet-name>
        <servlet-class>model.InsertComplain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Student_check</servlet-name>
        <servlet-class>model.Student_check</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>College_check</servlet-name>
        <url-pattern>/College_check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>College_details</servlet-name>
        <url-pattern>/College_details</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentRegis</servlet-name>
        <url-pattern>/StudentRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertComplain</servlet-name>
        <url-pattern>/InsertComplain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Student_check</servlet-name>
        <url-pattern>/stud_check</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
